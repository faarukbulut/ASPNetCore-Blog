
@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>INSPINIA | Static Tables</title>

    <link href="~/admin-tema/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/admin-tema/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/admin-tema/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="~/admin-tema/css/animate.css" rel="stylesheet">
    <link href="~/admin-tema/css/style.css" rel="stylesheet">

</head>

<body>

    <div id="wrapper">

        @await Html.PartialAsync("~/Views/Admin/AdminNavbarPartial.cshtml")

        <div id="page-wrapper" class="gray-bg">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                    </div>
                </nav>
            </div>
            <div class="row wrapper border-bottom white-bg page-heading">
                <div class="col-lg-10">
                    <h2>Static Tables</h2>
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a href="index.html">Home</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a>Tables</a>
                        </li>
                        <li class="breadcrumb-item active">
                            <strong>Static Tables</strong>
                        </li>
                    </ol>
                </div>
                <div class="col-lg-2">
                </div>
            </div>
            <div class="wrapper wrapper-content animated fadeInRight">
                @RenderBody()
            </div>
        </div>
    </div>



    <!-- Mainly scripts -->
    <script src="~/admin-tema/js/jquery-3.1.1.min.js"></script>
    <script src="~/admin-tema/js/popper.min.js"></script>
    <script src="~/admin-tema/js/bootstrap.js"></script>
    <script src="~/admin-tema/js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/admin-tema/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <!-- Peity -->
    <script src="~/admin-tema/js/plugins/peity/jquery.peity.min.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="~/admin-tema/js/inspinia.js"></script>
    <script src="~/admin-tema/js/plugins/pace/pace.min.js"></script>

    <!-- iCheck -->
    <script src="~/admin-tema/js/plugins/iCheck/icheck.min.js"></script>

    <!-- Peity -->
    <script src="~/admin-tema/js/demo/peity-demo.js"></script>

    <script>
        $(document).ready(function(){
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });
        });
    </script>

@*
    @await RenderSectionAsync("scripts")*@


</body>

</html>

